<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 实体类,简称 -设置别名 -->
	<typeAliases>
		<typeAlias alias="User" type="bupt.wspn.bean.User" />
		<typeAlias alias="Player" type="bupt.wspn.bean.Player" />
		<typeAlias alias="Match" type="bupt.wspn.bean.Match" />
		<typeAlias alias="MatchLog" type="bupt.wspn.bean.MatchLog" />
		<typeAlias alias="MatchWeek" type="bupt.wspn.bean.MatchWeek" />
		<typeAlias alias="Record" type="bupt.wspn.bean.Record" />
		
	</typeAliases>

	<!-- 实体接口映射资源 -->
	<!--
		说明：如果xxMapper.xml配置文件放在和xxMapper.java统一目录下，mappers也可以省略，因为org.mybatis.spring.mapper.MapperFactoryBean默认会去查找与xxMapper.java相同目录和名称的xxMapper.xml
	-->

</configuration>  